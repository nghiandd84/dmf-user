    "@babel/preset-typescript": "^7.10.4",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/core": "11.0.0",
    "@emotion/react": "11.1.4",
    "@emotion/styled": "11.0.0",
    "@touk/federated-types": "^1.1.0",
    "@types/react": "^17.0.2",
